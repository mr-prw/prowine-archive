<?xml version="1.0" encoding="utf-8"?>
<odoo>
		<record id="view_stock_config_settings_export_stock" model="ir.ui.view">
			<field name="name">Sale Order Check Stock Configuration</field>
			<field name="model">res.config.settings</field>
			<field name="inherit_id" ref="stock.res_config_settings_view_form" />
			<field name="arch" type="xml">
			<data>
				<div id="warning_info" position="after">
					<div class="col-xs-12 col-md-6 o_setting_box">
						<div class="o_setting_left_pane">
							<field name="group_visible_export_stock" string= "Export StockInfo in Excel file ?" class="oe_inline" />
						</div>
						<div class="o_setting_right_pane">
							<label for="group_visible_export_stock"/>
	                    </div>
	                </div>
	                <div class="col-xs-12 col-md-6 o_setting_box"/>
				</div>
				
			</data>
			</field>
		</record>
	
</odoo>